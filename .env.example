# MinIO 설정 (1차 목표)
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_NAME=nyc-taxi-data
MINIO_SECURE=False

# ClickHouse 설정 (1차 목표)
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9000
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_DATABASE=nyc_taxi

# Google Cloud 설정 (2차 목표)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/credentials.json
GCP_PROJECT_ID=your-project-id
GCP_BUCKET_NAME=your-bucket-name
GCP_DATASET_ID=your_dataset_id

# NYC Open Data API 설정
NYC_OPEN_DATA_APP_TOKEN=your_app_token

# Airflow 설정
AIRFLOW_HOME=/path/to/airflow
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__EXECUTOR=LocalExecutor

# 데이터 파이프라인 설정
DATA_START_DATE=2023-01-01
DATA_END_DATE=2023-12-31
TAXI_DATA_TYPE=yellow  # yellow, green, fhv

# 대시보드 설정 (최종 목표)
DASHBOARD_PORT=8050
